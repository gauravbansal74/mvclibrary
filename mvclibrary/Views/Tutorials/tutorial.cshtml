@{
    ViewBag.Title = "Offcampus4u : "+ @ViewBag.PageTitle;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main class="bs-docs-masthead-other" id="content" role="main" style="width:80%;margin:0 auto; ">

    <div class="panel panel-default"  style="width:25%;margin:0 auto;text-align:left;float:left;">
        <div class="panel-heading">
            <h3 class="panel-title"><b>All Tutorials</b></h3>
          </div>
       <ul class="nav nav-pills nav-stacked" style="max-width: 300px;">
            @foreach (var tutorial in Model.tutorialCategoryList)
               {
                <li role="presentation"><a href="@Url.Action("Tutorial", "Tutorials", new { id=tutorial.tutorialCategoryId})">@tutorial.tutorialCategoryName</a></li>
               }
        </ul>
    </div>
    <div class="panel panel-default"  style="width:70%;margin:0 auto;text-align:left;float:right;">
        <div class="well" style="margin:0 auto;width:100%; text-align:center;padding:0px;">
        <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
            <!-- offcampus4u_main2_AdSense1_728x90_as -->
            <ins class="adsbygoogle"
                    style="display:inline-block;width:728px;height:90px"
                    data-ad-client="ca-pub-7843509776288043"
                    data-ad-slot="1962217363"></ins>
            <script>
                (adsbygoogle = window.adsbygoogle || []).push({});
            </script>
      </div>
        <div class="panel-body">
             <h4 class="signup">
           @ViewBag.PageTitle
           <br />
           <small>Offcampus4u - Learn, Work & Earn</small>
       </h4>

       <div class="signupform">
           @{
               <ul class="list-group">
               @foreach (var category in Model.videoList)
               {
                <li class="list-group-item"><a href="@Url.Action("seeTutorial", "Tutorials", new { id=category.videoId,catId = category.categoryId})">@category.videoTitle</a></li>
               }
               </ul>

            }
        </div>
    </div>
        </div>

    <div style="clear:both;"></div>
</main>

